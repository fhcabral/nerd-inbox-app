{
  "expo": {
    "name": "Nerd Inbox",
    "slug": "nerd-inbox-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/nerdLogo.png",
    "scheme": "nerdinboxapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDisplayName": "Nerd Inbox"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/nerdLogo.png",
        "backgroundColor": "#0A0921"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fhcabral.nerdinboxapp"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/nerdLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/nerdLogo.png",
          "resizeMode": "cover",
          "backgroundColor": "#0A0921",
          "dark": {
            "backgroundColor": "#0A0921"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "root": "./src/app"
      },
      "eas": {
        "projectId": "a467fa8d-d1cc-4fc6-9c71-8ba56573a076"
      }
    }
  }
}